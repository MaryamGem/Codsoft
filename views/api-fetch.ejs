<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>JSON Fetch API's</title>
</head>
<body>
    <button id="todo" onclick="FetchTodo()">Fetch ToDo</button>
</body>
<script>
    // fetch todo function

    function FetchTodo(){
        // http request

        fetch("https://jsonplaceholder.typicode.com/todos/1")
         .then(function(data){
            console.log(data)
         })
    }
</script>
</html>